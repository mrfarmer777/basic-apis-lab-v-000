<h1>Search Github Repositories</h1>
<%= form_tag '/search' do %>
  <%= label_tag :query %>
  <%= text_field_tag :query %>
  <%= submit_tag "Search Github!" %>
<% end %>
<% if @repos %>
  <%@repose.each do |repo|%>
    <h2><a href="<%=repo.html_url%>"><%=repo.name%></a></h2>
    <p><%=repo.descprtion%></p>

